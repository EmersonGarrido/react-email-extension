{
  "name": "vscode-tsx",
  "displayName": "TSX",
  "description": "TSX preview, lint, compile for Visual Studio Code.",
  "version": "0.0.1",
  "publisher": "EmersonGarrido",
  "license": "MIT",
  "readme": "README.md",
  "icon": "images/icon.png",
  "author": {
    "name": "Emerson Garrido",
    "email": "emeersongarrido.dev@gmail.com"
  },
  "homepage": "https://github.com/EmersonGarrido/react-email-extension#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/EmersonGarrido/react-email-extension"
  },
  "bugs": {
    "url": "https://github.com/EmersonGarrido/react-email-extension/issues"
  },
  "galleryBanner": {
    "color": "#f45e43",
    "theme": "dark"
  },
  "categories": [
    "Other",
    "Linters",
    "Snippets",
    "Formatters",
    "Programming Languages"
  ],
  "keywords": [
    "vscode",
    "tsx",
    "preview",
    "lint",
    "syntax"
  ],
  "engines": {
    "vscode": "^1.60.0"
  },
  "activationEvents": [
    "onCommand:extension.previewTSX"
  ],
  "contributes": {
    "commands": [
      {
        "command": "extension.previewTSX",
        "title": "Preview JSX"
      }
    ]
  },
  "main": "./out/extension",
  "scripts": {
    "vscode:prepublish": "npm run build",
    "build": "node esbuild.js",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "lint": "npx eslint . --ext .js,.ts",
    "checks": "npm run lint && npm run compile",
    "pub": "npm run checks && vsce publish patch",
    "pub-minor": "npm run checks && vsce publish minor",
    "pub-major": "npm run checks && vsce publish major"
  },
  "devDependencies": {
    "@types/babel__core": "^7.1.20",
    "@types/copy-paste": "^1.1.30",
    "@types/html-minifier": "^4.0.1",
    "@types/js-beautify": "^1.13.2",
    "@types/mime": "^2.0.3",
    "@types/node": "^16.18.10",
    "@types/react-dom": "^18.0.9",
    "@types/vscode": "^1.60.0",
    "@typescript-eslint/eslint-plugin": "^4.31.0",
    "@typescript-eslint/parser": "^4.31.0",
    "esbuild": "^0.12.26",
    "eslint": "^7.32.0",
    "prettier": "^2.4.0",
    "typescript": "^4.4.2"
  },
  "dependencies": {
    "@babel/core": "^7.20.7",
    "@babel/preset-react": "^7.18.6",
    "@emotion/core": "^11.0.0",
    "@emotion/react": "^11.10.5",
    "@react-email/render": "^0.0.3",
    "copy-paste": "^1.3.0",
    "html-minifier": "^4.0.0",
    "js-beautify": "^1.14.0",
    "jsx-to-html": "^1.1.0",
    "mime": "^2.5.2",
    "react": "^18.2.0",
    "react-dom": "^16.2.0",
    "ts-jest": "^25.0.0",
    "vscode": "^1.1.37"
  }
}
